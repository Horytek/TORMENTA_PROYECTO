{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets @babel/preset-env",
    "dev": "nodemon index.js",
    "client": "cd ./client && npm run dev",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "dev:fullstack": "concurrently --names \"BACKEND,FRONTEND\" --prefix-colors \"cyan,magenta\" \"npm run dev\" \"npm run client\"",
    "build:client": "npm --prefix ./client ci && npm --prefix ./client run build",
    "start": "node index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@botpress/chat": "^0.5.1",
    "@botpress/webchat": "^3.2.7",
    "@heroui/use-theme": "^2.1.10",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.12.0",
    "node-cron": "^4.2.1",
    "openai": "^6.1.0",
    "react-google-places-autocomplete": "^4.1.0",
    "sequelize": "^6.37.5",
    "socket.io": "^4.8.1",
    "three": "^0.180.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/node": "^7.24.8",
    "@babel/preset-env": "^7.24.8",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4"
  }
}
