{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets @babel/preset-env",
    "dev": "nodemon --exec babel-node index.js",
    "client": "cd ./client && npm run dev",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "dev:fullstack": "concurrently --names \"BACKEND,FRONTEND\" --prefix-colors \"cyan,magenta\" \"npm run dev\" \"npm run client\"",
    "build": "npm install --prefix ./client",
    "start": "npm run build && babel-node index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@react-pdf/renderer": "^3.4.4",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "html2pdf.js": "^0.10.2",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "mysql2": "^3.12.0",
    "react-icons": "^5.4.0",
    "react-to-print": "^2.15.1",
    "sequelize": "^6.37.5",
    "zod": "^3.23.8"
  },
